{
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": false,
    "target": "ESNext",
    "jsx": "react",
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "importHelpers": true,
      "sourceMap": false,
          "module": "esnext",

    "paths": {
      "@/*": ["src/*"],
      "@/components": ["src/components"],
      "@/utils": ["src/utils"]
    }
  }
}
